parameters:
	level: 8
	paths:
		- app
		- tests
	ignoreErrors:
		-
			identifier: variable.undefined
			paths:
				- app/Lib/Router/
		-
			identifier: staticClassAccess.privateProperty
			paths:
				- app/Lib/Application/Container/AppContainer.php
				- app/Kernels//Kernel.php
		-
			identifier: new.static
			paths:
				- app/Kernels//Kernel.php
		-
			identifier: argument.type
			paths:
				- app/Lib/Helpers/ClassFinder.php
		-
			identifier: missingType.generics
			paths:
				- app/Lib/Helpers/ClassFinder.php
